 case "$1" in
  /dev/mmcblk*) p="${1}p$" ;;
  *) p="" ;;
 esac
 runknoppixlib /sbin/mkdosfs -F32 -n "KNOPPIX" "${1}${p}1" || return 3
 mount -t vfat "${1}${p}1" /mnt-user >/dev/null 2>&1 || return 4
 runknoppixlib /usr/bin/rsync -rP /mnt-system/boot /mnt-system/KNOPPIX /mnt-user/ || return 5
 if [ ! -r /mnt-user/boot/syslinux/syslinux.cfg -a -r /mnt-user/boot/isolinux/isolinux.cfg ]; then
  [ -d /mnt-user/boot/syslinux ] && rm -rf /mnt-user/boot/syslinux
  mv -f /mnt-user/boot/isolinux /mnt-user/boot/syslinux
  mv -f /mnt-user/boot/syslinux/isolinux.cfg /mnt-user/boot/syslinux/syslinux.cfg
  rm -f /mnt-user/boot/syslinux/isolinux.* /mnt-user/boot/syslinux/*.cat /mnt-user/boot/isolinux.* /mnt-user/boot/*.cat
 fi
 echo "2 /KNOPPIX-DATA reiserfs" > /mnt-user/KNOPPIX/knoppix-data.inf
 umount /mnt-user ; sleep 2
 runknoppixlib /usr/bin/ms-sys -s "$1" || return 6
 runknoppixchroot /usr/bin/syslinux "${1}${p}1" || return 7
 format_data "${1}${p}2" || return 8
